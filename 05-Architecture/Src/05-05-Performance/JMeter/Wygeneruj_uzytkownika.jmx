          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">String uuid = java.util.UUID.randomUUID().toString();
String json = &quot;{\n&quot;
			+ &quot;  \&quot;guid\&quot; : \&quot;&quot; + uuid + &quot;\&quot;,\n&quot;
			+ &quot;  \&quot;person\&quot; : {\n&quot;
			+ &quot;    \&quot;name\&quot; : \&quot;Jan\&quot;,\n&quot;
			+ &quot;    \&quot;surname\&quot; : \&quot;Fraudowski_&quot; + uuid + &quot;\&quot;,\n&quot;
			+ &quot;    \&quot;dateOfBirth\&quot; : \&quot;1980-01-01\&quot;,\n&quot;
			+ &quot;    \&quot;gender\&quot; : \&quot;Male\&quot;,\n&quot;
			+ &quot;    \&quot;nationalIdentificationNumber\&quot; : \&quot;2345678901\&quot;\n&quot;
			+ &quot;  }\n&quot;
			+ &quot;}&quot;;

vars.put(&quot;user_json&quot;, json);</stringProp>
        </BeanShellPreProcessor>
